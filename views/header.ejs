  <script src="/socket.io/socket.io.js"></script>
  <script>
  $(function(){
    var socket = io();
    socket.emit("fruit","apple");//전체
    socket.emit("MyId","Sunghwan");//특정 ID
    socket.emit("notice","오늘은 SocketIO 공부하는 날!");//나를제외한전체
    socket.on("fruit",function(data){
      $('.chatbox').append('<div class=\'friend-bubble bubble\'>'+ data +'</div>');
    })
    socket.on("MyId",function(data){
      //$('.container').append(data + "<br />");
    })
    socket.on("notice",function(data){
      //$(".container").append(data + "<br />")
    })
    $("#btnSend").click(function(){
      socket.emit("btnSend",$("#text").val());  
    })
    socket.on("msgReceive",function(data){
      $(".container").append(data + "<br />");
    })
  })
  </script>
